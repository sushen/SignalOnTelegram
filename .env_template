# Telegram Bot Token
BOT_TOKEN=your_telegram_bot_token

# Telegram User API credentials (if using user authentication)
API_ID=your_api_id
API_HASH=your_api_hash
GROUP_ID = your_GROUP_ID
PHONE_NUMBER = your_PHONE_NUMBER

# Optional: Other configurations
DATABASE_URL=your_database_url  # e.g., sqlite:///db.sqlite3 or a PostgreSQL/MySQL URL
REDIS_URL=your_redis_url        # e.g., redis://localhost:6379/0
LOG_LEVEL=INFO                  # Logging level (DEBUG, INFO, WARNING, ERROR, CRITICAL)
